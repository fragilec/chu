        var body = $response.body.replace(/"courseStatus":"lock"/g, '"courseStatus":"unlock"')
}
if (url.indexOf(path4) != -1) {
    var body = $response.body.replace(/"isLock":"1"/g, '"isLock":"0"');
}
if (url.indexOf(path5) != -1) {
    	obj.data["canListen"] = "1";
        body = JSON.stringify(obj);
}
if (url.indexOf(path6) != -1) {
        obj.data["membership"] = [{"title":"无限卡会员","type":"unlimited","desc":"","startTime":"2021-06-24T16:00:00.000+0000","endTime":"2024-12-31T15:59:59.000+0000","status":1}];
        body = JSON.stringify(obj);            
}
if (url.indexOf(path7) != -1) {
    	var body = $response.body.replace(/"lock":"1"/g, '"lock":"0"')
}
if (url.indexOf(path8) != -1) {
