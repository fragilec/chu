var body = $response.body;
var url = $request.url;
var obj = JSON.parse(body);

const path1 = '/student_app/auth/token_login';
const path2 = '/v1/goods/getAlbumGoodsDetail';
const path3 = '/v1/goods/getFilterAlbumCourseGoodsList';
